{{#if resume.skills}}
    <section class="skills-section section">
        <h2 class="section-title"><i class="fa fa-rocket"></i>Skills &amp; Proficiency</h2>
        <div class="skillset">
            {{#resume.skills}}
                <div class="item">
                    {{#if keywords}}
                        <h3 class="skill-name">{{name}}</h3>
                        {{#keywords}}
                            <span class="skill-keyword">{{this}}</span>
                        {{/keywords}}
                    {{else}}
                        <h3 class="level-title">{{name}}</h3>
                        <div class="level-bar">
                            <div class="level-bar-inner" data-level="{{skillLevel level}}">
                            </div>
                        </div><!--//level-bar-->
                    {{/if}}
                </div><!--//item-->
            {{/resume.skills}}

        </div>
    </section><!--//skills-section-->
{{/if}}